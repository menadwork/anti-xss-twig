  "keywords": [
    "html",
    "security",
    "twig",
    "extension",
    "XSS"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Lars Moelleken",
      "homepage": "http://www.moelleken.org/"
    }
  ],
  "autoload": {
    "psr-4": {
